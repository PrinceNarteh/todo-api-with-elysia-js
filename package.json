{
  "name": "bun-elysia-prisma",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/app.ts",
    "start": "bun run src/app.ts",
    "build": "bun build src/app.ts --outdir dist",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "test": "bun test",
    "lint": "bun run biome check src",
    "format": "bun run biome format src --write"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.4.2",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/swagger": "^1.3.1",
    "@sinclair/typebox": "^0.34.41",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "elysia": "latest",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.9",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.16.0",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.8",
    "prisma": "^7.1.0",
    "tsx": "^4.21.0"
  },
  "module": "src/index.js"
}
